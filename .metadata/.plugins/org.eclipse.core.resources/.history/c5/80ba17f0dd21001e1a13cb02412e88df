package exercicioSituacaoAcademica;

public class Aluno extends Humano{
	private double media;

	private boolean statusDeAprovacao;
	private int faltas;
	
	public Aluno(double media, boolean statusDeAprovacao, int faltas) {
		super();
		this.media = media;
		this.statusDeAprovacao = statusDeAprovacao;
		this.faltas = faltas;
	}
	
	
}
