package CesarPOO.atividade03;


public class Main {

	public static void main(String[] args) {
		int[] lista = new int[50];
		
		for (int c = 0; c<51; c++) {
			
			
			try {
				lista[c] = c;
				lista[c] = "alÃ´";
				System.out.println(lista[c]);
			} catch(Exception e) {
				System.out.println("ERRO! " + e);
			}
			
		
			
		}
		
		
	}

}
