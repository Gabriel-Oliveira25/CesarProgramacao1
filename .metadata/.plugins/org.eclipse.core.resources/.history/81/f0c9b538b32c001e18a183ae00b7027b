package cesarPOO.atividade03;

public class Universidade extends Object{
	private String nome;
	
	public Universidade() {
		
	}
	
	public Universidade(String nome) {
		this.nome = nome;
	}
	
	@Override
	public String toString() {
		return nome;
	}
	
	@Override
	public boolean equals(String nome) {
		return nome == this.nome;
	}
	
	

}
