package CodigoDiscord;

public class Relatorio {
	
	public void relatorio(MeioDeTransporte[] meioTrans) {
		
		
		for(int contador = 0; contador < meioTrans.length; contador++) {
			
			meioTrans[contador].getNome();
			
			
		}
		
		
		
		for (MeioDeTransporte meioDeTransporte : meioTrans) {
			
			System.out.println("NOME: " + meioDeTransporte.getNome());
			System.out.println("VELOCIDADE: " + meioDeTransporte.getVelocidade());

		}
	}
}
